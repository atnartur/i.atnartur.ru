<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">

    <title>{{name}}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <meta name="description" content="{{description}}" />
    <meta name="keywords" content="{{keywords}}" />

    <link rel="stylesheet" href="dist/base.min.css">
    <link rel="stylesheet" href="https://atnartur.ru/libs/font-awesome/css/font-awesome.min.css">

    <!-- Schema.org markup (Google) -->
    <meta itemprop="name" content="{{name}}">
    <meta itemprop="description" content="{{description}}">
    <meta itemprop="image" content="{{mainPhoto}}">

    <!-- Twitter Card markup-->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="@atnartur">
    <meta name="twitter:title" content="{{name}}">
    <meta name="twitter:description" content="{{description}}">
    <meta name="twitter:creator" content="@atnartur">
    <!-- Twitter summary card with large image must be at least 280x150px -->
    <meta name="twitter:image" content="{{mainPhoto}}">
    <meta name="twitter:image:alt" content="{{description}}">

    <!-- Open Graph markup (Facebook, Pinterest) -->
    <meta property="og:title" content="{{description}}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://i.atnartur.ru/" />
    <meta property="og:image" content="{{mainPhoto}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="og:site_name" content="{{description}}" />

    {{#each appleIcons}}
        <link rel="apple-touch-icon" sizes="{{this}}x{{this}}" href="dist/images/photo_{{this}}.png">
    {{/each}}
    {{#each icons}}
        <link rel="icon" type="image/png" sizes="{{this}}x{{this}}"  href="dist/images/photo_{{this}}.png">
    {{/each}}

    <meta name="msapplication-TileColor" content="#F0F0F0">
    <meta name="theme-color" content="#F0F0F0">
</head>

<body>
<header>
    <div class="logo">
        <div class="container">
            <div class="info row">
                <div class="col-md-3">
                    <img src="dist/images/photo_top.png" srcset="dist/images/photo_top.png 1x, dist/images/photo_top@2x.png 2x" alt="Артур Атнагулов" id="photo">
                </div>
                <div class="col-md-9">
                    <h1>
                        <a href="/">
                            Атнагулов Артур
                            <small>@atnartur</small>
                        </a>
                    </h1>
                    <h2>
                        <span>Веб-разработчик</span> /
                        <span><i class="fa fa-globe"></i> Казань</span>
                    </h2>

                    <ul id="socials">
                        <li class="write">Написать:</li>
                        <li>
                            <a href="http://telegram.me/atnartur" title="Написать в Telegram" target="_blank">
                                <i class="fa fa-send"></i>
                            </a>
                        </li>
                        <li>
                            <a href="http://vk.com/atnartur" title="Я ВКонтакте" target="_blank">
                                <i class="fa fa-vk"></i>
                            </a>
                        </li>
                        <li>
                            <a href="http://twitter.com/atnartur" title="Мой Твиттер - @atnartur" target="_blank">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a href="http://instagram.com/atnartur" title="Мой Инстаграм - @atnartur" target="_blank">
                                <i class="fa fa-instagram"></i>
                            </a>
                        </li>
                        <li>
                            <a href="http://github.com/atnartur" title="GitHub - @atnartur" target="_blank">
                                <i class="fa fa-github"></i>
                            </a>
                        </li>
                        <li class="delimiter">/</li>
                        <li>
                            <a href="mailto:i@atnartur.ru" title="Напишите мне - i@atnartur.ru" class="bold">
                                i<i class="fa fa-at"></i>atnartur.ru
                            </a>
                        </li>
                        <li class="delimiter">/</li>
                        <li><a href="http://atnartur.ru" class="bold"><i class="fa fa-rss"></i> Почитать блог</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

</header>

<div class="container" id="main">
    <div class="posts">
        <article class="row article" id="work">
            <div class="col-md-3 pos-sticky">
                <h1>Работаю в</h1>
                <i class="fa fa-briefcase header-icon"></i>
            </div>
            <div class="col-md-9">
                {{#each work}}
                    <div class="row work">
                        <div class="col-md-2 time">{{time}}</div>
                        <div class="col-md-3 img">
                            <a href="{{link}}" target="_blank">
                                <img src="{{image}}" id="{{imageId}}" height="{{imageHeight}}" alt="{{imageAlt}}">
                            </a>
                        </div>
                        <div class="col-md-7">{{info}}</div>
                    </div>
                {{/each}}
            </div>
        </article>
        <article class="row article" id="tech">
            <div class="col-md-3 pos-sticky">
                <h1>Технологии и инструменты</h1>
                <i class="fa fa-code header-icon"></i>
            </div>
            <div class="col-md-9">
                <h2>Frontend</h2>
                <p>
                    <b>Языки:</b> HTML 5, CSS 3, JavaScript (ES6)<br>
                    <b>Библиотеки:</b> VueJS, React, ThreeJS<br>
                    <b>Сборка:</b> webpack, gulp
                </p>
                <h2>Backend</h2>
                <p>
                    <b>Технологии:</b>
                        <b>Python</b> (Django, Flask, Celery),
                        <b>PHP</b> (Phalcon, Laravel),
                        <b>NodeJS</b> (electron, socket.io),
                        <b>C#, Java</b> <br>
                    <b>Базы данных:</b> PosgreSQL, MySQL, Redis, RabbitMQ
                </p>
                <h2>Инструменты</h2>
                <p>Git, Intellij IDEA, Docker, Sentry, Gitlab CI</p>
            </div>
        </article>
        <article class="row article" id="projects">
            <div class="col-md-3 pos-sticky">
                <h1>Проекты</h1>
                <i class="fa fa-file-text-o header-icon"></i>
            </div>
            <div class="col-md-9">
                <div class="list-group">
                    {{#each projects}}
                        <a href="#project-{{slug}}"  class="list-group-item">
                            <h3 class="list-group-item-heading">
                                {{name}}

                                {{#each tags}}
                                    <span class="label label-project-tag">{{this}}</span>
                                {{/each}}
                            </h3>
                            <p class="list-group-item-text">{{short}}</p>
                        </a>
                    {{/each}}
                </div>
            </div>
        </article>
        <article class="row article" id="blog_latest" style="display: none;">
            <div class="col-md-3 pos-sticky">
                <h1>Последнее из блога</h1>
                <i class="fa fa-rss header-icon"></i>
            </div>
            <div class="col-md-9">
                <div class="list-group" id="blog_results"></div>
                <a href="http://atnartur.ru" target="_blank" class="btn btn-primary pull-right">
                    <i class="fa fa-external-link"></i> Читать все статьи
                </a>
            </div>
        </article>
        <article class="row article" id="interests">
            <div class="col-md-3 pos-sticky">
                <h1>Интересы</h1>
                <i class="fa fa-folder-o header-icon"></i>
            </div>
            <div class="col-md-9">
                <p>Некоторые вещи, которыми я занимаюсь (или занимался) в свободное время.</p>
                <ul class="list-group">
                    <li class="list-group-item">
                        <b><i class="fa fa-video-camera"></i> Видеомонтаж:</b>
                        <a href=https://www.youtube.com/watch?v=ez6dfVJMnYw" target="_blank">видео на трек RUBINGOOD - JAVA</a>,
                        <a href="https://vk.com/wall-153470393_16" target="_blank">концерт "День первокурсника ИТИС 2017"</a>
                    </li>
                    <li class="list-group-item">
                        <b>
                            <i class="fa fa-music"></i>
                            <a href="http://vk.com/atnartur_music" target="_blank">atnartur.music</a>
                        </b>
                    </li>
                </ul>
            </div>
        </article>
    </div>
</div>
<footer>
    <div class="container">
        <p>
            &copy; 2011-{{year}} <a href="/" class="logo">Артур Атнагулов</a>
            <br>
            <a href="https://github.com/atnartur/i.atnartur.ru" target="_blank">
                <i class="fa fa-code"></i>
            </a>
        </p>
    </div>
</footer>

{{#each projects}}
    <div class="modal fade project-view" id="project-{{slug}}" tabindex="-1" role="dialog" aria-labelledby="{{slug}}">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h1 class="modal-title">
                        {{name}}

                        <small>
                            {{#each tags}}
                                <span class="label label-project-tag">{{this}}</span>
                            {{/each}}
                        </small>

                        {{#if site}}
                            <a href="{{site}}" target="_blank" class="btn btn-primary project-link btn-lg pull-right">
                                <i class="fa fa-link"></i> Перейти к проекту
                            </a>
                        {{/if}}
                    </h1>
                </div>
                <div class="modal-body">
                    <div class="row">
                        {{#if images}}
                            <div class="col-md-4">
                                <div id="project-{{slug}}-images" class="carousel slide" data-ride="carousel">
                                    <!-- Wrapper for slides -->
                                    <div class="carousel-inner" role="listbox">
                                        {{#each images}}
                                            <a href="dist/images/projects/{{../slug}}/{{this}}" target="_blank"
                                                class="item {{#unless @key}}active{{/unless}}">
                                                <div class="item-content">
                                                    <img src="dist/images/projects/{{../slug}}/{{this}}" alt="{{name}}">
                                                </div>
                                            </a>
                                        {{/each}}
                                    </div>

                                    <!-- Controls -->
                                    <a class="left carousel-control" href="#project-{{slug}}-images" role="button" data-slide="prev">
                                        <i class="fa fa-arrow-left" aria-hidden="true"></i>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="right carousel-control" href="#project-{{slug}}-images" role="button" data-slide="next">
                                        <i class="fa fa-arrow-right" aria-hidden="true"></i>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>
                            </div>
                            <div class="col-md-8">
                        {{/if}}

                        {{#unless images}}
                            <div class="col-md-12">
                        {{/unless}}

                            <p>{{short}}</p>

                            {{{full}}}

                            {{#if tech}}
                                <p>
                                    <b><i class="fa fa-code"></i> Технологии:</b>
                                    {{#if tech_str}}{{tech_str}}{{/if}}
                                    {{#each tech}}
                                        <br><b>{{@key}}:</b> {{this}}
                                    {{/each}}
                                </p>
                            {{/if}}

                            {{#if years}}
                                <p><b><i class="fa fa-clock-o"></i> Сроки реализации:</b> {{years}}</p>
                            {{/if}}

                        {{#unless images}}
                            </div>
                        {{/unless}}

                        {{#if images}}
                            </div>
                        {{/if}}
                    </div>

                </div>
            </div>
        </div>
    </div>
{{/each}}


<script src="https://yastatic.net/jquery/2.1.3/jquery.min.js"></script>
<script src="https://yastatic.net/bootstrap/3.3.1/js/bootstrap.min.js"></script>
<script src="dist/js/main.js"></script>

